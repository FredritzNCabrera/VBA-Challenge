Sub TickerChecker()    Dim Ticks As String    Dim TicksVolume As Double            TicksVolume = 0            Dim Ticks_data_row As Integer            Ticks_data_row = 2        Dim OpenPrice As Double    Dim ClosePrice As Double    Dim YearlyChange As Double    Dim PercentChange As Double        Dim GreatVolume As Double    Dim GreatIncrease As Double    Dim GreatDecrease As Double        Cells(1, 9).Value = "Ticker"    Cells(1, 10).Value = "Yearly Change"    Cells(1, 11).Value = "Percent Change"    Cells(1, 12).Value = "Total Stock Volume"        Cells(1, 16).Value = "Ticker"    Cells(1, 17).Value = "Value"    Cells(2, 15).Value = "Greatest % Increase"    Cells(3, 15).Value = "Greatest % Decrease"    Cells(4, 15).Value = "Greatest Total Volume"        lastrow = Cells(Rows.Count, 1).End(xlUp).Row        For i = 2 To lastrow                If Cells(i + 1, 1).Value <> Cells(i, 1).Value Then                Ticks = Cells(i, 1).Value                TicksVolume = TicksVolume + Cells(i, 7).Value                    Range("I" & Ticks_data_row).Value = Ticks            Range("L" & Ticks_data_row).Value = TicksVolume                ClosePrice = Cells(i, 6).Value        YearlyChange = (ClosePrice - OpenPrice)                    Range("J" & Ticks_data_row).Value = YearlyChange                    If (OpenPrice = 0) Then                                PercentChange = 0                            Else                                PercentChange = YearlyChange / OpenPrice                            End If                            Range("K" & Ticks_data_row).Value = PercentChange                Range("K" & Ticks_data_row).NumberFormat = "0.00%"                                    Ticks_data_row = Ticks_data_row + 1                                    TicksVolume = 0                                        OpenPrice = Cells(i + 1, 3)                                Else                                    TicksVolume = TicksVolume + Cells(i, 7).Value                                End If                                        Next i                            Lastrow_DataTable = Cells(Rows.Count, 9).End(xlUp).Row                                For i = 2 To Lastrow_DataTable                                    If Cells(i, 10).Value > 0 Then                                            Cells(i, 10).Interior.ColorIndex = 4                                            Else                                            Cells(i, 10).Interior.ColorIndex = 3                                            End If                                Next i                    Lastrow_DataTable = Cells(Rows.Count, 9).End(xlUp).Row                GreatVolume = Cells(2, 12).Value        GreatIncrease = Cells(2, 11).Value        GreatDecrease = Cells(2, 11).Value                    For i = 2 To Lastrow_DataTable                            If Cells(i, 12).Value > GreatVolume Then                                        GreatVolume = Cells(i, 12).Value                                        Cells(4, 16).Value = Cells(i, 9).Value                                    Else                                    GreatVolume = GreatVolume                                End If                                If Cells(i, 11).Value > GreatIncrease Then                                    GreatIncrease = Cells(i, 11).Value                                    Cells(2, 16).Value = Cells(i, 9).Value                                Else                                    GreatIncrease = GreatIncrease                                End If                                If Cells(i, 11).Value < GreatDecrease Then                                    GreatDecrease = Cells(i, 11).Value                                    Cells(3, 16).Value = Cells(i, 9).Value                                Else                         GreatDecrease = GreatDecrease                                End If                                    Cells(2, 17).Value = Format(GreatIncrease, "Percent")                    Cells(3, 17).Value = Format(GreatDecrease, "Percent")                    Cells(4, 17).Value = Format(GreatVolume, "Scientific")                                Next i     End Sub